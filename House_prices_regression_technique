al_imputer = SimpleImputer(strategy='mean')\n","\n","for col in combined.columns:\n","    if combined[col].isna().any():\n","        if combined[col].dtype == 'object' or combined[col].dtype == 'string':\n","            combined[col] = categorical_imputer.fit_transform(combined[[col]]).ravel()\n","        else:\n","            combined[col] = numerical_imputer.fit_transform(combined[[col]]).ravel()"],"metadata":{"id":"zZBfwVY25Juc"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["from sklearn.preprocessing import LabelEncoder\n","\n","label_encoders = {}\n","\n","for col in combined.columns:\n","    if combined[col].dtype == 'object' or combined[col].dtype == 'string':\n","        label_encoders[col] = LabelEncoder()\n","        combined[col] = label_encoders[col].fit_transform(combined[col])"],"metadata":{"id":"ypVAi3846oCp"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["combined.drop_duplicates(inplace=True)\n","\n","train = combined[combined['is_train'] == 1].drop(columns=['is_train'])\n","test = combined[combined['is_train'] == 0].drop(columns=['is_train'])"],"metadata":{"id":"BFbdlUr26vu0"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["train.head()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":255},"id":"xYgGy3Ws62Ze","executionInfo":{"status":"ok","timestamp":1732783093814,"user_tz":-330,"elapsed":739,"user":{"displayName":"Rambha Devi","userId":"14711144264830641669"}},"outputId":"01dd7326-dce6-461b-ef40-91c38277965b"},"execution_count":null,"outputs":[{"output_type":"execute_result","data":{"text/plain":["   Id  MSSubClass  MSZoning  LotFrontage  LotArea  Street  Alley  LotShape  \\\n","0   1          60         3         65.0     8450       1      0         3   \n","1   2          20         3         80.0     9600       1      0         3   \n","2   3          60         3         68.0    11250       1      0         0   \n","3   4          70         3         60.0     